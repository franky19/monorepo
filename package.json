{
  "name": "monorepo",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "admin": "cd apps/admin && yarn dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"*/.{js,jsx,ts,tsx,md}\"",
    "eslint": "eslint \"{apps,packages/ui}/*/.{js,jsx,ts,tsx}\"",
    "check-format": "prettier --check --ignore-path=.prettierignore \"{apps,packages/ui}/*/.{js,jsx,ts,tsx}\"",
    "static-code-analysis": "yarn eslint --fix && tsc && yarn check-format",
    "dynamic-code-analysis": "jest --coverage",
    "test": "yarn static-code-analysis && yarn dynamic-code-analysis --passWithNoTests",
    "postinstall": "husky install"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@turbo/gen": "^1.10.6",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "autoprefixer": "^10.4.14",
    "axios": "1.3.6",
    "cookie": "^0.5.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "md5-typescript": "^1.0.5",
    "moment": "^2.29.4",
    "mysql2": "^3.2.4",
    "postinstall-postinstall": "2.1.0",
    "prettier": "^2.8.8",
    "tailwind-config": "*",
    "turbo": "^1.10.6",
    "typescript": "^5.1.6",
    "typescript-config": "*"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "crypto-js": "4.2.0",
    "firebase": "^10.11.1",
    "react-json-tree": "^0.18.0",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "1.14.0",
    "yarn": "^1.22.19"
  }
}
